  "main": "n/a",
  "scripts": {
    "start:lambda": "netlify-lambda serve src/lambda",
    "build:lambda": "netlify-lambda build src/lambda",
    "build": "gatsby clean && GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true CI=1 gatsby build  --log-pages",
    "build-serve": "yarn build && gatsby serve",
    "develop-watch": "npm run develop",
    "build-main": "rm -rf .cache public",
    "develop": "NODE_OPTIONS=--max-old-space-size=8192 ./node_modules/.bin/gatsby develop -H 0.0.0.0",
    "form-css-upload": "node-sass --watch src/components/ContentfulModules/ModuleFormIFrame.module.scss static/form-style/FormModuleUpload.css",
    "form-css-overrides": "node-sass --watch src/components/ContentfulModules/ModuleFormIFrameOverrides.module.scss static/form-style/FormModule.css",
    "postinstall": "npx rimraf node_modules/errlop/edition-browsers"
  },
  "apollo": {
    "service": {
      "endpoint": {
        "url": "http://localhost:8000/___graphql?"
      }
    }
  },
  "browserslist": [
    "> 1%",
    "IE >= 11",
    "last 2 versions"
  ],
  "devDependencies": {
    "netlify-lambda": "^2.0.15",
    "npm-run-all": "^4.1.5",
